---
import Albums from '@/components/pages/albums.astro';
import { getAlbums } from '@/lib/content';

export async function getStaticPaths() {
  const albums = await getAlbums();

  return albums.map(({ slug }) => {
    return {
      params: {
        slug: slug?.current,
      },
    };
  });
}
