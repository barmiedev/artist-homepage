{
  "name": "artist-homepage",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:node": "astro build --node",
    "preview": "astro preview --node",
    "astro": "astro",
    "format": "biome format --write ./src",
    "lint": "biome lint --write ./src",
    "check": "biome check --write ./src",
    "prepare": "husky",
    "sanity:typegen": "bunx sanity schema extract --path ./src/sanity/schema.json --enforce-required-fields && bunx sanity typegen generate",
    "email": "email dev"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/netlify": "^5.5.4",
    "@astrojs/react": "^3.6.2",
    "@react-email/components": "0.0.27",
    "@sanity/astro": "^3.1.6",
    "@sanity/client": "^6.21.3",
    "@sanity/image-url": "^1.0.2",
    "@sanity/visual-editing": "^2.1.9",
    "astro": "^4.15.4",
    "astro-portabletext": "^0.10.0",
    "date-fns": "^4.1.0",
    "groq": "^3.62.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^4.0.1-alpha.0",
    "sanity": "^3.57.1",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@astrojs/node": "^8.3.4",
    "@biomejs/biome": "1.8.3",
    "@portabletext/types": "^2.0.13",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "husky": "^9.1.5",
    "react-email": "3.0.2",
    "vite": "^5.4.3"
  }
}
